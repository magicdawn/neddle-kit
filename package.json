{
  "name": "needle-kit",
  "version": "3.2.1",
  "description": "Arya's needle, little but powerful.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "vitest run",
    "prepare": "husky install",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm test && pnpm build",
    "typeswrong": "attw --pack ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicdawn/needle-kit.git"
  },
  "keywords": [
    "needle",
    "kit"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/needle-kit/issues"
  },
  "homepage": "https://github.com/magicdawn/needle-kit",
  "dependencies": {
    "@humanfs/node": "^0.16.3",
    "fs-extra": "^11.2.0",
    "shell-escape": "^0.2.0",
    "untildify": "^5.0.0"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.4",
    "@swc/core": "^1.6.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "@types/shell-escape": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "unplugin-auto-import": "^0.17.6",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
